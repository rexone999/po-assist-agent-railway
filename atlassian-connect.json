{
  "key": "po-assist-agent",
  "name": "PO Assist Agent",
  "baseUrl": "https://po-assist-agent-gemini-production-1766.up.railway.app",
  "authentication": {
    "type": "none"
  },
  "lifecycle": {
    "installed": "/installed"
  },
  "scopes": [
    "read",
    "write"
  ],
